<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.birthdatecalculator.MainController">
   <children>
       <Button layoutX="34.0" layoutY="96.0" onAction="#calculate" text="Расчитать" />
      <Label layoutX="36.0" layoutY="28.0" text="Дата рождения" />
      <DatePicker fx:id="datePicker" layoutX="34.0" layoutY="57.0" />
      <Label layoutX="34.0" layoutY="150.0" text="Миссия:" />
      <Label layoutX="34.0" layoutY="185.0" text="Сфера:" />
      <TextField fx:id="missionTF" editable="false" layoutX="165.0" layoutY="145.0" />
      <TextField fx:id="sphereTF" editable="false" layoutX="165.0" layoutY="180.0" />
      <Label layoutX="34.0" layoutY="220.0" text="Ощущение Счастья" />
      <TextField fx:id="happinessTF" editable="false" layoutX="165.0" layoutY="215.0" />
      <Label layoutX="34.0" layoutY="255.0" text="Психическая Энергия" />
      <TextField fx:id="psyTF" editable="false" layoutX="165.0" layoutY="250.0" />
      <Label layoutX="34.0" layoutY="290.0" text="Философия" />
      <TextField fx:id="philTF" editable="false" layoutX="165.0" layoutY="285.0" />
      <Label layoutX="36.0" layoutY="325.0" text="Код Фин Успеха" />
      <TextField fx:id="codeTF" editable="false" layoutX="165.0" layoutY="320.0" />
      <Button layoutX="254.0" layoutY="57.0" mnemonicParsing="false" onAction="#createCalendar" text="Календарь" />
   </children>
</AnchorPane>
